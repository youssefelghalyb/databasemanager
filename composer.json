{
    "name": "youssef-elghaly/database-manager",
    "description": "A Laravel package for managing database migrations and seeders for modular applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Youssef Elghaly",
            "email": "yousef.elghalyb@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0",
        "nwidart/laravel-modules": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "YoussefElghaly\\DatabaseManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YoussefElghaly\\DatabaseManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YoussefElghaly\\DatabaseManager\\DatabaseManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
